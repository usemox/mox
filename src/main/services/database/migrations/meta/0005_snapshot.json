      },
      "name": "threads",
      "isExisting": false,
      "definition": "WITH latest_emails AS (\n  SELECT \n    e.*,\n    (SELECT MIN(value) FROM json_each(e.label_ids) WHERE value LIKE 'CATEGORY_%') AS category,\n    ROW_NUMBER() OVER (PARTITION BY e.thread_id ORDER BY e.date DESC) AS rn\n  FROM emails e\n  WHERE e.folder = 'INBOX' \n)\nSELECT\n  id,\n  thread_id,\n  headers,\n  from_address,\n  to_address,\n  subject,\n  snippet,\n  date,\n  unread,\n  synced_at,\n  label_ids,\n  draft,\n  folder,\n  category\nFROM latest_emails\nWHERE rn = 1\nORDER BY date DESC\n"
    }
  },
