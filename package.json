{
  "name": "mox",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "d": "doppler run --",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "start": "pnpm run d electron-vite preview",
    "dev": "pnpm run d electron-vite dev",
    "build": "pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm run build && electron-builder --dir",
    "build:win": "pnpm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:export": "drizzle-kit export > ./resources/schema.sql"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.1.16",
    "@ai-sdk/openai": "^1.1.13",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@google-cloud/pubsub": "^4.10.0",
    "@libsql/client": "^0.14.0",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.4",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tanstack/react-router": "^1.102.1",
    "@tanstack/router-devtools": "^1.102.1",
    "@tiptap/extension-link": "^2.11.5",
    "@tiptap/pm": "^2.11.5",
    "@tiptap/react": "^2.11.5",
    "@tiptap/starter-kit": "^2.11.5",
    "@types/tinycolor2": "^1.4.6",
    "ai": "^4.1.45",
    "cmdk": "^1.1.1",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.3.9",
    "google-auth-library": "^9.15.1",
    "googleapis": "^144.0.0",
    "keytar": "^7.9.0",
    "linkedom": "^0.18.9",
    "marked-react": "^3.0.0",
    "mobx": "^6.13.6",
    "mobx-react-lite": "^4.1.0",
    "motion": "^12.5.0",
    "prebuild-install": "^7.1.3",
    "react-virtuoso": "^4.12.6",
    "tw-animate-css": "^1.2.5",
    "ulid": "^2.3.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
