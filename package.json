    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/rebuild": "^3.7.1",
    "@tailwindcss/postcss": "^4.0.4",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/router-plugin": "^1.102.1",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.0",
